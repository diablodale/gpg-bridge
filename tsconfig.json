{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "shared",
		"lib": [
			"ES2022"
		],
		"types": ["node"],
		"sourceMap": true,
		"rootDir": "shared",
		"skipLibCheck": true,
		"strict": true
	},
	"include": ["shared/**/*"],
	"exclude": ["node_modules", "agent-proxy", "request-proxy"]
}
